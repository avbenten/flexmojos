    <component id="List" class="flex.component.List"/>
    <component id="MoveSegment" class="flex.graphics.MoveSegment"/>
    <component id="MXMLComponent" class="flex.core.MXMLComponent"/>
    <component id="Panel" class="flex.component.Panel"/>

    <component id="TextGraphic" class="flex.graphics.TextGraphic"/>
    <component id="TextInput" class="flex.component.TextInput"/>
    <component id="TextView" class="flex.component.TextView"/>
    <component id="Transform" class="flex.geom.Transform"/>
